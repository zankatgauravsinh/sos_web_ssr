    "@ngxs/store": "^3.8.2",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "environments": "^1.2.7",
    "express": "^4.18.2",
    "ng-select2-component": "^10.0.0",
    "ngx-image-zoom": "^2.1.0",
    "ngx-owl-carousel-o": "^18.0.0",
    "ngx-toastr": "^18.0.0",
    "rxjs": "~7.8.0",
    "swiper": "^8.4.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
  },
  "overrides": {
    "@ngxs/devtools-plugin": {
      "@angular/core": "$@angular/core"
    },
    "@ngxs/form-plugin": {
      "@angular/core": "$@angular/core",
      "@angular/forms": "$@angular/forms"
    },
    "@ngxs/logger-plugin": {
      "@angular/core": "$@angular/core"
    },
    "@ngxs/router-plugin": {
      "@angular/core": "$@angular/core",
      "@angular/router": "$@angular/router"
    },
    "@ngxs/storage-plugin": {
      "@angular/core": "$@angular/core"
    },
    "@ngxs/store": {
      "@angular/core": "$@angular/core"
    }
